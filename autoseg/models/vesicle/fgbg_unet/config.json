{
	"model":{

		"name": "fgbg_unet",
		"params": {
			"in_channels": 1,
			"num_fmaps": 12,
			"fmap_inc_factor": 5,
			"downsample_factors": [[2, 2], [2, 2], [2, 2]],
			"kernel_size_down": [
				[[3, 3], [3, 3]], 
				[[3, 3], [3, 3]], 
				[[3, 3], [3, 3]], 
				[[3, 3], [3, 3]]
			],
			"kernel_size_up": [
				[[3, 3], [3, 3]], 
				[[3, 3], [3, 3]], 
				[[3, 3], [3, 3]]
			],
			"activation": "ReLU",
			"num_fmaps_out": null,
			"num_heads": 1,
			"constant_upsample": true,
			"padding": "valid"
		},
		"optimizer": {
			"name": "Adam",
			"params": {
				"lr": 0.5e-4
			}
		},
		"output_shapes": [1],
