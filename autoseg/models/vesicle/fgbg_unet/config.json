			},
			"simple": {
			},
			"noise": {},
			"intensity": {
				"scale_min": 0.9,
				"scale_max": 1.1,
				"shift_min": -0.1,
				"shift_max": 0.1
			}
		},
		"nodes":{
		}
	},
	"predict":{
		"keys":{
			"input": {
				"RAW": 1
			},
			"output": {
			    	"PRED_MASK": 1
			}
		}
	},
	"segment":{
		"method": "watershed"
	}
}
